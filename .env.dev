# Configuration Environnement DEV - Hytale Web Manager
# Ne PAS commit ce fichier en production

# Serveur Hytale
MEMORY=4G
INIT_MEMORY=
MAX_MEMORY=
SERVER_PORT=5520
SERVER_HOST=0.0.0.0
TZ=Europe/Paris
UID=1000
GID=1000
ENABLE_AOT=false
JVM_OPTS=
EXTRA_ARGS=
DEBUG=true                    # Mode debug activé en dev

# Backend
NODE_ENV=development
PORT=4000
CONTAINER_NAME=hytale-server

# Frontend
VITE_API_URL=http://localhost:4000
VITE_WS_URL=ws://localhost:4000

# Options de dev
ENABLE_VERBOSE_LOGS=true      # Logs détaillés
ENABLE_MOCK_DATA=false        # Utiliser des données mockées si besoin
DEV_MODE=true                 # Fonctionnalités expérimentales activées
